pipeline{
    agent any
   
    stages{
        stage('git checkout'){
            steps{
                git 'https://github.com/omarmerrou/spring-boot-helloworld.git'
            }
        }
        stage('build the application'){
            steps{
                bat 'mvn clean install'
            }
            
        }
        stage(’Unit Test Execution’) {
            steps{
                bat ’mvn test’
            }
       }
    }
}
